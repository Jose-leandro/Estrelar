generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"

  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model refratores {
  id            Int      @id @default(autoincrement())
  nome_refrator String
  preco         String   @unique
  img_url       String
  createdAt     DateTime @default(now())
}
